{
    "name": "api-project",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --fix --cache",
        "test": "jest",
        "build": "tsc",
        "build:dev": "tsc --watch",
        "start": "node --enable-source-maps dist/server.js",
        "start:dev": "nodemon --watch dist/",
        "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/amaliacicala/api-project.git"
    },
    "bugs": {
        "url": "https://github.com/amaliacicala/api-project/issues"
    },
    "homepage": "https://github.com/amaliacicala/api-project#readme",
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "concurrently": "^7.3.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "nodemon": "^2.0.19",
        "prettier": "2.7.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1"
    }
}
